{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "myChurchTemplate": {
      "root": "",
      "sourceRoot": "src",
      "projectType": "application",
      "prefix": "app",
      "schematics": {},
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/myChurchTemplate",
            "index": "src/index.html",
            "main": "src/main.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "src/tsconfig.app.json",
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ],
            "styles": [
              "./src/assets/css/plugins.css",
              "./src/revolution/css/settings.css",
              "./src/revolution/css/layers.css",
              "./src/revolution/css/navigation.css",
              "./src/assets/css/backgrounds.css",
              "./src/assets/css/style-red.css",
              "./src/styles.css"
            ],
            "scripts": [
              "./src/assets/js/modernizr.js",
              "./src/assets/js/jquery.js",
              "./src/assets/js/jquery.cookie.js",
              "./src/assets/js/foundation.js",
              "./src/assets/js/classie.js",
              "./src/assets/js/owl.carousel.js",
              "./src/assets/js/jquery.validate.js",
              "./src/assets/js/jquery.countdown.js",
              "./src/assets/js/jquery.counterup.js",
              "./src/assets/js/jquery.scrollTo.js",
              "./src/assets/js/parallax.js",
              "./src/assets/js/jquery.waypoints.js",
              "./src/assets/js/jquery.flexslider.js",
              "./src/revolution/js/jquery.themepunch.tools.min.js",
              "./src/revolution/js/jquery.themepunch.revolution.min.js",
              "./src/assets/js/application.js",
              "./src/assets/js/mobileAppInit.js"
            ]
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "myChurchTemplate:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "myChurchTemplate:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "myChurchTemplate:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/test.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "src/tsconfig.spec.json",
            "karmaConfig": "src/karma.conf.js",
            "styles": [
              "src/styles.css"
            ],
            "scripts": [],
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ]
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "src/tsconfig.app.json",
              "src/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "myChurchTemplate-e2e": {
      "root": "e2e/",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "e2e/protractor.conf.js",
            "devServerTarget": "myChurchTemplate:serve"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": "e2e/tsconfig.e2e.json",
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    }
  },
  "defaultProject": "myChurchTemplate"
}